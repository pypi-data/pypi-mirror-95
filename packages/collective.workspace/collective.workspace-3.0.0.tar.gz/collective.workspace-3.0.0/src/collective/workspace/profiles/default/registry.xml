      <value_type type="plone.registry.field.Tuple">
        <value_type type="plone.registry.field.TextLine" />
      </value_type>
    </field>
    <value>
      <element key="Members">
        <element>Contributor</element>
        <element>Reader</element>
        <element>TeamMember</element>
      </element>
      <element key="Guests">
        <element>TeamGuest</element>
      </element>
      <element key="Admins">
        <element>Contributor</element>
        <element>Editor</element>
        <element>Reader</element>
        <element>Reviewer</element>
        <element>TeamManager</element>
      </element>
    </value>
  </record>
</registry>
