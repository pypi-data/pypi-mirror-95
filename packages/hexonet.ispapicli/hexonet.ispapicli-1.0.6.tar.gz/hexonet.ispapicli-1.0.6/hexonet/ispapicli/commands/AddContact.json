{"command": "AddContact", "description": "Create a contact handle to use with a domain name.", "availability": "All users have access to this command.", "paramaters": [{"Parameter": "COMMAND", "Min": "1", "Definition": "AddContact", "Type": "COMMAND"}, {"Parameter": "NAME", "Min": "1", "Definition": "Name, separated by space to fill FIRSTNAME and LASTNAME", "Type": "TEXT or NULL"}, {"Parameter": "FIRSTNAME", "Min": "0", "Definition": "Firstname", "Type": "TEXT or NULL"}, {"Parameter": "LASTNAME", "Min": "0", "Definition": "Lastname", "Type": "TEXT or NULL"}, {"Parameter": "STREET", "Min": "1", "Definition": "Street", "Type": "TEXT"}, {"Parameter": "CITY", "Min": "1", "Definition": "City", "Type": "TEXT"}, {"Parameter": "ZIP", "Min": "1", "Definition": "ZIP / Postal Code", "Type": "TEXT"}, {"Parameter": "COUNTRY", "Min": "1", "Definition": "ISO-3166 Country Code", "Type": "COUNTRY"}, {"Parameter": "PHONE", "Min": "1", "Definition": "ITU Phone Number", "Type": "PHONE or NULL"}, {"Parameter": "EMAIL", "Min": "1", "Definition": "Email Address", "Type": "EMAIL"}, {"Parameter": "CONTACT", "Min": "0", "Definition": "Contact Handle ID", "Type": "CONTACT"}, {"Parameter": "AUTH", "Min": "0", "Definition": "Authcode / Password", "Type": "TEXT"}, {"Parameter": "TITLE", "Min": "0", "Definition": "Title", "Type": "TEXT or NULL"}, {"Parameter": "MIDDLENAME", "Min": "0", "Definition": "Middlename", "Type": "TEXT or NULL"}, {"Parameter": "ORGANIZATION", "Min": "0", "Definition": "Organization / Company Name", "Type": "TEXT or NULL"}, {"Parameter": "STATE", "Min": "0", "Definition": "State / Province", "Type": "TEXT or NULL"}, {"Parameter": "FAX", "Min": "0", "Definition": "ITU Phone Number", "Type": "PHONE or NULL"}, {"Parameter": "NEW", "Min": "0", "Definition": "Force to create a new contact even if a contact with the same data exists", "Type": "0, 1 or NULL"}, {"Parameter": "STREET[0..N]", "Min": "0", "Definition": "Multiple Street Lines", "Type": "TEXT or NULL"}, {"Parameter": "VATID", "Min": "0", "Definition": "Store a VATID", "Type": "TEXT or NULL"}, {"Parameter": "IDNUMBER", "Min": "0", "Definition": "Store a ID Number", "Type": "TEXT or NULL"}, {"Parameter": "IDAUTHORITY", "Min": "0", "Definition": "Authority that has issued the contact's personal or organizational identification number", "Type": "TEXT or NULL"}, {"Parameter": "DISCLOSE", "Min": "0", "Definition": "Set to 1 to disclose (publish) the contact details, set to 0 for non-disclosure. This feature is only supported by dedicated registries and by the HEXONET whois server", "Type": "0, 1"}, {"Parameter": "DISCLOSE-ELEMENTS", "Min": "0", "Definition": "Specify which dedicated contact details should be affected by the DISCLOSE parameter setting. If not stated the DISCLOSE parameter affects all possible contact details. Multiple values are possible and have to be separated by a blank space. Possible values are:NAME: disclose the nameORG: disclose the organization nameADDR: disclose street, state, zip, city and countryVOICE: disclose the phone numberFAX: disclose the fax numberEMAIL: disclose the email address", "Type": "`/^(((NAME"}, {"Parameter": "X-AFNIC-DISCLOSE", "Min": "0", "Definition": "If set to 1 the contact details will get disclosed for domains registered at AFNIC", "Type": "0, 1 or NULL"}, {"Parameter": "X-CAT-DISCLOSE-FLAG", "Min": "0", "Definition": "Global disclose flag", "Type": "0 or 1"}, {"Parameter": "X-CAT-DISCLOSE-TYPE[0..N]", "Min": "0", "Definition": "Disclose contact elements", "Type": "VOICE or EMAIL"}, {"Parameter": "X-CAT-EMAIL-SPONSOR", "Min": "0", "Definition": "Email address of the sponsoring contact", "Type": "TEXT or NULL"}, {"Parameter": "X-CAT-MAINTAINER", "Min": "0", "Definition": "Maintainer", "Type": "TEXT or NULL"}, {"Parameter": "X-CAT-LANG", "Min": "0", "Definition": "Contact language", "Type": "TEXT or NULL"}, {"Parameter": "X-COOP-SPONSOR[0..N]", "Min": "0", "Definition": "Sponsoring contact", "Type": "TEXT or NULL"}, {"Parameter": "X-DE-TYPE", "Min": "0", "Definition": "Type of contact at the .DE registry", "Type": "PERSON, ORGANIZATION, ROLE or NULL"}]}