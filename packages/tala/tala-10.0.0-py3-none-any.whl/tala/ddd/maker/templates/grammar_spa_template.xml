<?xml version="1.0" encoding="utf-8"?>
<grammar>

  <action name="top">
    <one-of>
      <item>empezar</item>
      <item>vuelve a empezar</item>
      <item>vuelve a comenzar</item>
      <item>vuelve al inicio</item>
      <item>reinicia</item>
    </one-of>
  </action>

  <action name="up">
    <one-of>
      <item>atrás</item>
      <item>regresa</item>
      <item>vuelve atrás</item>
      <item>vuelve</item>
      <item>cancela</item>
    </one-of>
  </action>

</grammar>
