{
  "bs_html_parser": "html5lib",
  "timeout": 1200,
  "actions": {
    "abort": {
      "close": true
    }
  },
  "scraping": {
    "all": true,
    "disable_animations": true,
    "attempts": 3,
    "prescroll": false,
    "page_load_timeout": 60,
    "wait_loading": 1.0,
    "wait_scroll": 0.1,
    "wait_action": 0.1,
    "mhtml_timeout": 10,
    "save_mhtml": false,
    "temp_path": "~/.webtraversallibrary/",
    "history": true,
    "full_history": true
  },
  "scrolling": {
    "max_page_height": 2000
  },
  "browser": {
    "browser": "chrome",
    "useragent": "Mozilla/5.0 (iPhone; CPU iPhone OS 14_3 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/14.0 Mobile/15E148 Safari/604.1",
    "width": 375,
    "height": 812,
    "pixelRatio": 3,
    "headless": false,
    "enable_mhtml": false,
    "proxy": ""
  },
  "javascript": {
    "info": "debug",
    "warning": "debug",
    "severe": "error"
  },
  "debug": {
    "autoscroll": true,
    "default_canvas_viewport": true,
    "live": true,
    "live_delay": 1.0,
    "live_annotation": true,
    "screenshots": false,
    "save": false,
    "preserve_window": false,
    "action_highlight_color": "#FFB3C7"
  }
}
