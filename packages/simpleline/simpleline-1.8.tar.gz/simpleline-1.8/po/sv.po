#: ../simpleline/render/adv_widgets.py:42
msgid "Error"
msgstr "Fel"

#: ../simpleline/render/adv_widgets.py:51
#, python-format
msgid "Press %s to exit"
msgstr "Tryck %s för att avsluta"

#: ../simpleline/render/adv_widgets.py:70
msgid "Password"
msgstr "Lösenord"

#: ../simpleline/render/adv_widgets.py:71
msgid "Enter your passphrase"
msgstr "Ange din lösenfras"

#: ../simpleline/render/adv_widgets.py:84
msgid "Passphrase: "
msgstr "Lösenfras: "

#: ../simpleline/render/adv_widgets.py:121
msgid "Question"
msgstr "Fråga"

#: ../simpleline/render/adv_widgets.py:131
#, python-format
msgid "Please respond '%(yes)s' or '%(no)s'"
msgstr "Svara ”%(yes)s” eller ”%(no)s”"

#: ../simpleline/render/adv_widgets.py:167
msgid "Help"
msgstr "Hjälp"

#: ../simpleline/render/adv_widgets.py:173
msgid "The help is not available."
msgstr "Hjälpen är inte tillgänglig."

#: ../simpleline/render/adv_widgets.py:186
#, python-format
msgid "Press %s to return"
msgstr "Tryck %s för att återgå"

#. Default message of the prompt
#: ../simpleline/render/prompt.py:32
msgid "Please make a selection from the above"
msgstr "Gör ett val från ovanstående"

#. String to use in a prompt when we want users to press the key ENTER.
#: ../simpleline/render/prompt.py:35
msgid "ENTER"
msgstr "RETUR"

#: ../simpleline/render/prompt.py:39
msgid "to quit"
msgstr "för att avsluta"

#: ../simpleline/render/prompt.py:43
msgid "to continue"
msgstr "för att fortsätta"

#: ../simpleline/render/prompt.py:47
msgid "to refresh"
msgstr "för att uppdatera"

#: ../simpleline/render/prompt.py:51
msgid "to help"
msgstr "för hjälp"
