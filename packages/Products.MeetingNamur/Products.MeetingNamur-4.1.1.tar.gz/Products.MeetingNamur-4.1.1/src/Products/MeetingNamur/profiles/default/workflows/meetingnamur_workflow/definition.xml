<?xml version="1.0"?>
<dc-workflow workflow_id="meetingnamur_workflow"
             title="meetingnamur_workflow"
             state_variable="review_state"
             initial_state="created"
             xmlns:i18n="http://xml.zope.org/namespaces/i18n"
             i18n:domain="plone">
 <permission>View</permission>
 <permission>Access contents information</permission>
 <permission>Modify portal content</permission>
 <permission>Review portal content</permission>
 <permission>Delete objects</permission>
 <permission>PloneMeeting: Add annex</permission>
 <state state_id="closed"
        title="closed"
        i18n:attributes="title">
  <exit-transition transition_id="backToDecided"/>
  <permission-map name="Access contents information"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
   <permission-role>MeetingObserverGlobal</permission-role>
   <permission-role>Reader</permission-role>
  </permission-map>
  <permission-map name="Delete objects"
                  acquired="False">
   <permission-role>Manager</permission-role>
  </permission-map>
  <permission-map name="Modify portal content"
                  acquired="False">
   <permission-role>Manager</permission-role>
  </permission-map>
  <permission-map name="PloneMeeting: Add annex"
                  acquired="False">
  </permission-map>
  <permission-map name="Review portal content"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
  </permission-map>
  <permission-map name="View"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
   <permission-role>MeetingObserverGlobal</permission-role>
   <permission-role>Reader</permission-role>
  </permission-map>
 </state>
 <state state_id="created"
        title="created"
        i18n:attributes="title">
  <exit-transition transition_id="freeze"/>
  <permission-map name="Access contents information"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
   <permission-role>MeetingObserverGlobal</permission-role>
   <permission-role>Reader</permission-role>
  </permission-map>
  <permission-map name="Delete objects"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
  </permission-map>
  <permission-map name="Modify portal content"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
  </permission-map>
  <permission-map name="PloneMeeting: Add annex"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
  </permission-map>
  <permission-map name="Review portal content"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
  </permission-map>
  <permission-map name="View"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
   <permission-role>MeetingObserverGlobal</permission-role>
   <permission-role>Reader</permission-role>
  </permission-map>
 </state>
 <state state_id="decided"
        title="decided"
        i18n:attributes="title">
  <exit-transition transition_id="backToFrozen"/>
  <exit-transition transition_id="close"/>
  <permission-map name="Access contents information"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
   <permission-role>MeetingObserverGlobal</permission-role>
   <permission-role>Reader</permission-role>
  </permission-map>
  <permission-map name="Delete objects"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
  </permission-map>
  <permission-map name="Modify portal content"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
  </permission-map>
  <permission-map name="PloneMeeting: Add annex"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
  </permission-map>
  <permission-map name="Review portal content"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
  </permission-map>
  <permission-map name="View"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
   <permission-role>MeetingObserverGlobal</permission-role>
   <permission-role>Reader</permission-role>
  </permission-map>
 </state>
 <state state_id="frozen"
        title="frozen"
        i18n:attributes="title">
  <exit-transition transition_id="backToCreated"/>
  <exit-transition transition_id="decide"/>
  <permission-map name="Access contents information"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
   <permission-role>MeetingObserverGlobal</permission-role>
   <permission-role>Reader</permission-role>
  </permission-map>
  <permission-map name="Delete objects"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
  </permission-map>
  <permission-map name="Modify portal content"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
  </permission-map>
  <permission-map name="PloneMeeting: Add annex"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
  </permission-map>
  <permission-map name="Review portal content"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
  </permission-map>
  <permission-map name="View"
                  acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>MeetingManager</permission-role>
   <permission-role>MeetingObserverGlobal</permission-role>
   <permission-role>Reader</permission-role>
  </permission-map>
 </state>
 <transition transition_id="backToCreated"
             title="backToCreated"
             new_state="created"
             trigger="USER"
             before_script=""
             after_script=""
             i18n:attributes="title">
  <action url="" icon="" category="workflow" i18n:translate="">backToCreated</action>
  <guard>
    <guard-expression>python:here.wfConditions().mayCorrect()</guard-expression>
  </guard>
 </transition>
 <transition transition_id="backToDecided"
             title="backToDecided"
             new_state="decided"
             trigger="USER"
             before_script=""
             after_script=""
             i18n:attributes="title">
  <action url="" icon="" category="workflow" i18n:translate="">backToDecided</action>
  <guard>
    <guard-expression>python:here.wfConditions().mayCorrect()</guard-expression>
  </guard>
 </transition>
 <transition transition_id="backToFrozen"
             title="backToFrozen"
             new_state="frozen"
             trigger="USER"
             before_script=""
             after_script=""
             i18n:attributes="title">
  <action url="" icon="" category="workflow" i18n:translate="">backToFrozen</action>
  <guard>
    <guard-expression>python:here.wfConditions().mayCorrect()</guard-expression>
  </guard>
 </transition>
 <transition transition_id="close"
             title="close"
             new_state="closed"
             trigger="USER"
             before_script=""
             after_script=""
             i18n:attributes="title">
  <action url="" icon="" category="workflow" i18n:translate="">close</action>
  <guard>
    <guard-expression>python:here.wfConditions().mayClose()</guard-expression>
  </guard>
 </transition>
 <transition transition_id="decide"
             title="decide"
             new_state="decided"
             trigger="USER"
             before_script=""
             after_script=""
             i18n:attributes="title">
  <action url="" icon="" category="workflow" i18n:translate="">decide</action>
  <guard>
    <guard-expression>python:here.wfConditions().mayDecide()</guard-expression>
  </guard>
 </transition>
 <transition transition_id="freeze"
             title="freeze"
             new_state="frozen"
             trigger="USER"
             before_script=""
             after_script=""
             i18n:attributes="title">
  <action url="" icon="" category="workflow" i18n:translate="">freeze</action>
  <guard>
    <guard-expression>python:here.wfConditions().mayFreeze()</guard-expression>
  </guard>
 </transition>
 <variable variable_id="action" for_catalog="False"
           for_status="True" update_always="True">
  <description>The last transition</description>
  <default>
   <expression>transition/getId|nothing</expression>
  </default>
  <guard>
  </guard>
 </variable>
 <variable variable_id="actor" for_catalog="False"
           for_status="True" update_always="True">
  <description>The ID of the user who performed the last transition</description>
  <default>
   <expression>user/getId</expression>
  </default>
  <guard>
  </guard>
 </variable>
 <variable variable_id="comments" for_catalog="False"
           for_status="True" update_always="True">
  <description>Comments about the last transition</description>
  <default>
   <expression>python:state_change.kwargs.get('comment', '')</expression>
  </default>
  <guard>
  </guard>
 </variable>
 <variable variable_id="review_history" for_catalog="False"
           for_status="False" update_always="False">
  <description>Provides access to workflow history</description>
  <default>
   <expression>state_change/getHistory</expression>
  </default>
  <guard>
   <guard-permission>Request review</guard-permission>
   <guard-permission>Review portal content</guard-permission>
  </guard>
 </variable>
 <variable variable_id="time" for_catalog="False"
           for_status="True" update_always="True">
  <description>Time of the last transition</description>
  <default>
   <expression>state_change/getDateTime</expression>
  </default>
  <guard>
  </guard>
 </variable>
<!-- ##code-section FOOT -->
<!-- ##/code-section FOOT -->
 <!-- We dont use workflow scripts any more, we use subscribers -->
</dc-workflow>
